<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.lrms.entity.Student" table="student">
		<id name="id" column="id" type="int">
			<generator class="identity"> 
			</generator>
		</id>
		<property name="no" column="no" type="string"></property>
		<property name="name" column="name" type="string"></property>
		<property name="courseName" column="courseName" type="string"></property>
		<property name="phone" column="phone" type="string"></property>
		<property name="pwd" column="pwd" type="string"></property>
	</class>
</hibernate-mapping>

